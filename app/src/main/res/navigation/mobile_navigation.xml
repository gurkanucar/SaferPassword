<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_passwords">

    <fragment
        android:id="@+id/navigation_passwords"
        android:name="com.gucarsoft.saferpassword.Views.Fragments.passwords.PasswordsFragment"
        android:label="My Passwords"
        tools:layout="@layout/fragment_passwords" />

    <fragment
        android:id="@+id/navigation_add"
        android:name="com.gucarsoft.saferpassword.Views.Fragments.add.AddFragment"
        android:label="Add Password"
        tools:layout="@layout/fragment_add" />

    <fragment
        android:id="@+id/navigation_settings"
        android:name="com.gucarsoft.saferpassword.Views.Fragments.settings.SettingsFragment"
        android:label="Settings"
        tools:layout="@layout/fragment_settings" />
</navigation>